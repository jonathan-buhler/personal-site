    "author": "Jonathan Buhler <benedict.buhler@gmail.com>",
    "license": "GPL-3.0",
    "private": true,
    "dependencies": {
        "@types/animejs": "^3.1.0",
        "@types/lodash": "^4.14.150",
        "animejs": "^3.1.0",
        "lodash": "^4.17.15",
        "parcel": "^1.12.4"
    },
    "devDependencies": {
        "gh-pages": "^2.2.0",
        "husky": "^4.2.3",
        "prettier": "^2.0.2",
        "pretty-quick": "^2.0.1",
        "sass": "^1.26.3",
        "typescript": "^3.8.3"
    },
    "scripts": {
        "dev": "parcel index.html",
        "build": "parcel build index.html",
        "clear": "rm -rf .cache dist",
        "rebuild": "yarn run clear && yarn run build",
        "deploy": "yarn run rebuild && gh-pages -d dist -b master"
    },
    "prettier": {
        "tabWidth": 4,
        "quoteProps": "consistent",
        "printWidth": 120
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn pretty-quick --staged"
        }
    },
    "browserslist": [
        "last 1 Chrome versions"
    ]
}
